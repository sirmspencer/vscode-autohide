// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/*.js": false,
    "**/*.g.dart": false,
    "**/*.gr.dart": false,
    "lib/gen": false,
    "**/*.freezed.dart": false,
    "**/.*": false,
    "**/README.md": false,
    "**/analysis_options.yaml": false,
    "**/android": false,
    "**/build": false,
    "**/fb-account-service.json": false,
    "**/firebase.json": false,
    "**/firebase_options.dart": false,
    "**/firestore.indexes.json": false,
    "**/firestore.rules": false,
    "**/generated_plugin_registrant.dart": false,
    "**/ios": false,
    "**/linux": false,
    "**/macos": false,
    "**/pubspec.lock": false,
    "**/test": false,
    "**/web": false,
    "**/windows": false,
    "**syncfolder**": false,
